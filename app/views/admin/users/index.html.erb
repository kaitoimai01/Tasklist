<h1>ユーザー一覧</h1>
 <table class="table table-striped">
  <thead>
   <tr>
    <th>ID</th>
    <th>名前</th>
    <th>メールアドレス</th>
    <th>管理者権限</th>
    <th>登録日時</th>
    <th>更新日時</th>
   </tr>
  </thead>
  <tbody>
   <% @users.each do |user| %>
   <tr>
    <td><%= link_to user.id, edit_admin_user_path(user) %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.admin %></td>
    <td><%= user.created_at %></td>
    <td><%= user.updated_at %></td>
   </tr>
   <% end %>
  </tbody>
 </table>